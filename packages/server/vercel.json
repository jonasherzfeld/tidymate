{
  "builds": [
    {
      "src": "src/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.py",
      "headers": {
        "Access-Control-Allow-Origin": "https://tidymate.vercel.app"
      }
    }
  ],
  "env": {
    "PYTHONPATH": "src/"
  }
}
